#!/bin/sh

########################################################
#                      _ _
#  _ __  ___  __ _  __| (_)
# | '_ \/ __|/ _` |/ _` | |
# | |_) \__ \ (_| | (_| | |
# | .__/|___/\__,_|\__,_|_|
# |_|
## Name   : P S, Adithya
## email  : adithya3494@gmail.com
## gitlab : https://github.com/psadi
########################################################

ZSH_DOTFILES_DIR="${HOME}/Developer/dotfiles/zsh"

source "${ZSH_DOTFILES_DIR}/$(uname -a | awk '{print $1}').zshrc"
source "${ZSH_DOTFILES_DIR}/.zsh/_zsh.zsh"

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/adithyaps/anaconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/adithyaps/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/adithyaps/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/adithyaps/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

